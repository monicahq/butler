{
    "name": "monicahq/butler",
    "type": "project",
    "description": "The second version of Monica, personal CRM.",
    "keywords": [
        "framework",
        "crm",
        "contacts"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/coxy121/Laravel-JS-Localization.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "dev-master",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^7.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "livewire/livewire": "^1.0",
        "parsedown/laravel": "^1.2",
        "spatie/laravel-activitylog": "^3.1",
        "tightenco/ziggy": "^0.8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.4",
        "graham-campbell/testbench-core": "^3.1",
        "johnkary/phpunit-speedtrap": "^3.1",
        "mariuzzo/laravel-js-localization": "dev-laravel-7",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^8.5",
        "vimeo/psalm": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/AccountHelper.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
